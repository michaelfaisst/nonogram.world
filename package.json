        "@types/prettier": "^2.7.2",
        "eslint": "^8.38.0",
        "prettier": "^2.8.7",
        "prettier-plugin-tailwindcss": "^0.2.7",
        "turbo": "1.9.1",
        "typescript": "^5.0.4"
    }
